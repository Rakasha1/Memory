<Window x:Class="WPFApp.HighscoresWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFApp"
        mc:Ignorable="d"
        Title="Highscores" Height="700" Width="1000">
    <Grid>
        <StackPanel Name="labelStack" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="160,90,0,0">
        </StackPanel>
        <Label x:Name="NameLabel" Content="Names" HorizontalAlignment="Left" Margin="160,64,0,0" VerticalAlignment="Top" FontSize="24"/>
        <Label x:Name="ScoreLabel" Content="Scores" HorizontalAlignment="Left" Margin="332,64,0,0" VerticalAlignment="Top" FontSize="24"/>
        <Label x:Name="CardAmountLabel" Content="Amount of cards" HorizontalAlignment="Left" Margin="508,64,0,0" VerticalAlignment="Top" FontSize="24"/>
        <Button x:Name="StartMenuButton" Content="Next" HorizontalAlignment="Left" Margin="124,517,0,0" VerticalAlignment="Top" Click="StartMenuButton_Click" Height="35" Width="78"/>
    </Grid>
</Window>
